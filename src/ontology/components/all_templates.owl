        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0000-0002-5111-7263</obo:IAO_0000117>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychometric study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002030 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00002030">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00002013"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of human or animal activity, in terms of motivation, direction, result, emotion, perception, or etiology.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0000-0002-5111-7263</obo:IAO_0000117>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002031 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00002031">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00002013"/>
