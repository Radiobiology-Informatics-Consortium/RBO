        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of internal or external radiation on cellular or organismal physiology.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physiological study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000090 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000090">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation on the integrity, composition and quality of adult, fetal or embryonic anatomical structures.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">anatomical study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000091 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000091">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of external irradiation on an organism.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">external exposure study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000092 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000092">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of internal irradiation on an organism where the radiation source is internalised within the body tissues ( eg. by injection), gastrointestinal ( eg gavage), urogenital, auditory or respiratory ( eg instillation or inhalation) systems.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">internal contamination study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000093 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000093">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation exposure where the exposoure has an internal and external mode of application.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mixed exposure route study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000094 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000094">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000091"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of external irradiation on an organism where the most of the dose is given in a single or small number of fractions closely in time.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">acute external exposure study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000095 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000095">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000091"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of external irradiation on an organism where  the dose is given over a protracted time period either continuously or in many fractions. Often these are lose dose studies.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">chronic external exposure study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000096 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000096">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000092"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of internal irradiation on an organism where the radiation source is internalised within the body tissues ( eg. by injection), gastrointestinal ( eg gavage), urogenital, auditory or respiratory ( eg instillation or inhalation) systems. The dose is delivered over a long period of time, often continuously and generlly at low dose. For example long term Radon exposure by inhalation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">chronic internal contamination study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000097 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000097">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000092"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of internal irradiation on an organism where the radiation source is internalised within the body tissues ( eg. by injection), gastrointestinal ( eg gavage), urogenital, auditory or respiratory ( eg instillation or inhalation) systems. Most of the dose is given over a short period of time and is generally unfractionated, such as in brachytherapy.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">acute internal contamination study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000098 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000098">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000093"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation exposure where the exposoure has an internal and external mode of application. Acute exposure involves usually a single exposure over a short period of time with no fractionation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">acute mixed exposure route study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000099 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000099">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000093"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation exposure where the exposoure has an internal and external mode of application.  Chronic exposure involves usually a continuous or highly fractionated exposure over a long period of time.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">chronic mixed exposure route study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000100 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000100">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation on gene expression.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">gene expression study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000101 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000101">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation on the proteome or individual proteins.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">proteomics study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000102 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000102">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study that aims at the discovery of protein, RNA or other molecular entities or modifcations that can act as reliable surrogates for the prediction or measurement of the effects of radiation or the response of the organism to radiation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">marker discovery study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000103 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000103">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study that aims at the discovery, development or assessment of efficacy of therapeutic agents or processes designed to mitigate or cure the effects of radiation exposure.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">therapeutics study</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A owl:deprecated</owl:deprecated>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000104 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000104">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study that uses survival as an endpoint. Generally applicable to animal studies rather than human studies which would use the &quot;lifespan&quot; measure (RBO:00000085)</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">survival study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000105 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000105">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation on the metabolome or individual metabolites.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">metabolomics study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000106 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000106">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the contributing factors consequences and prevention of nuclear accidents</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">nuclear accident study</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A owl:deprecated</owl:deprecated>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000107 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000107">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the toxicity of chemical or biological agents on cells or whole organisms.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">toxicity study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000108 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000108">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000089"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effects of radiation on the endocrine system or of endocrine hormones on the effects of radiation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">endocrine study</rdfs:label>
    </owl:Class>
    


