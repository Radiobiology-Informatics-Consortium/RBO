    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000084 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000084">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the impact of radiation on the composition of or processes within the natural or anthropogenic environment.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">environmental study</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A owl:deprecated</owl:deprecated>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000085 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000085">
