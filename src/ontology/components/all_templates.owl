    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002025 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00002025">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00002013"/>
