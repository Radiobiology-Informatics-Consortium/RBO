        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mixed ion and photon irradiation study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00015025 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00015025">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00015024"/>
