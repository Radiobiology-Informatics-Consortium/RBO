        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0000-0002-5111-7263</obo:IAO_0000117>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">political and psephological study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002029 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00002029">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00002013"/>
