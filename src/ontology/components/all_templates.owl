        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fluence</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_010011 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_010011">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The flux of cosmic ray muons per cm2 at the surface of the Earth is approximately 1.0.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The quotient of dN by dt, where dN is the increment of the particle number in the time interval dt. The unit for flux is reciprocal second (s�1.)</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jack Miller</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCRP Glossary (https://ncrponline.org/wp-content/themes/ncrp/PDFs/NCRP-Composite-Glossary.pdf)</obo:IAO_0000119>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">flux</rdfs:label>
    </owl:Class>
    


