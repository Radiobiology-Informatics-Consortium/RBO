    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002026 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00002026">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00002013"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Study concerning exchange or transmission of thoughts, messages, or information between people or between authorities and the general public (for example educational information, risk communication, safety communication) concerning exposure to and use of radiation.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0000-0002-5111-7263</obo:IAO_0000117>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">communication study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002027 -->

