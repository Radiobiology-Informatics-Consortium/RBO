        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">study modality</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00002022 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00002022">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
