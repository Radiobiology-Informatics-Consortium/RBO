        <obo:IAO_0000111 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">spaceflight</obo:IAO_0000111>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Travel beyond the Earth&apos;s atmosphere</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jack Miller</obo:IAO_0000117>
