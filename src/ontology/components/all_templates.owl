        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study who endpoint is the lifespan of the organism under investifgation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">lifespan study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000086 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000086">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the effect of agents including radiation, seperately or together, on the incidence or type of neoplasia or pre-neoplasia.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">carcinogenesis study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000087 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000087">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study in which the structural or processual damage to normal tissue caused by radiation is assessed. Often by histopathology or molecular profiling.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tissue damage study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000088 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/RBO_00000088">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RBO_00000082"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A study of the mechanism or effects of radiation on dna damage and its repair.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dna damage and repair study</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/RBO_00000089 -->

