    <!-- http://purl.obolibrary.org/obo/GO_0000720 -->

    <Class rdf:about="http://purl.obolibrary.org/obo/GO_0000720">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0006289"/>
